<?php
session_start();
if (isset($_POST["add"])){
  if (isset($_POST["a"])) {
    $_SESSION["a"] = "hey";
    $_SESSION["aa"] = $_POST["aa"];
    }
  if (isset($_POST["b"])) {
    $_SESSION["b"] = "hey";
    $_SESSION["ab"] = $_POST["ab"];
    }
  if (isset($_POST["c"])) {
    $_SESSION["c"] = "hey";
    $_SESSION["ac"] = $_POST["ac"];
    }
  if (isset($_POST["d"])) {
    $_SESSION["d"] = "hey";
    $_SESSION["ad"] = $_POST["ad"];
    }
  if (isset($_POST["e"])) {
    $_SESSION["e"] = "hey";
    $_SESSION["ae"] = $_POST["ae"];
    }
  if (isset($_POST["f"])) {
    $_SESSION["f"] = "hey";
    $_SESSION["af"] = $_POST["af"];
    }
  if (isset($_POST["g"])) {
    $_SESSION["g"] = "hey";
    $_SESSION["ag"] = $_POST["ag"];
    }
  if (isset($_POST["h"])) {
    $_SESSION["h"] = "hey";
    $_SESSION["ah"] = $_POST["ah"];
    }
}
if (isset($_POST["subtract"])){
  if (isset($_POST["a"])) {
    unset($_SESSION["a"]);
    unset($_SESSION["aa"]);
    }
  if (isset($_POST["b"])) {
    unset($_SESSION["b"]);
    unset($_SESSION["ab"]);
    }
  if (isset($_POST["c"])) {
    unset($_SESSION["c"]);
    unset($_SESSION["ac"]);
    }
  if (isset($_POST["d"])) {
    unset($_SESSION["d"]);
    unset($_SESSION["ad"]);
    }
  if (isset($_POST["e"])) {
    unset($_SESSION["e"]);
    unset($_SESSION["ae"]);
    }
  if (isset($_POST["f"])) {
    unset($_SESSION["f"]);
    unset($_SESSION["af"]);
    }
  if (isset($_POST["g"])) {
    unset($_SESSION["g"]);
    unset($_SESSION["ag"]);
    }
  if (isset($_POST["h"])) {
    unset($_SESSION["h"]);
    unset($_SESSION["ah"]);
    }
};
?>

<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1 ">
