7.1-----------------------------
a) 
SELECT *
FROM LEERLINGEN
WHERE GEB_DATUM IN (
  SELECT MIN(GEB_DATUM)
  FROM LEERLINGEN );
  
SELECT *
FROM LEERLINGEN
WHERE GEB_DATUM IN (
  SELECT MAX(GEB_DATUM)
  FROM LEERLINGEN );
b)
GEB_DATUM IN omdat er mensen op dezelfde dag jarig kunnen zijn
c)
SELECT b.TITEL
FROM EXEMPLAREN e, BOEKEN b
WHERE e.STATUS = "in reparatie"
  AND e.BOEKNR = b.BOEKNR
  AND b.RUBRIEK = "Nederlands";
d)
SELECT VOORNAAM, TUSSENVOEGSEL, ACHTERNAAM, AUTEURNR DISTINCT
FROM AUTEURS
WHERE AUTEURNR IN (
  SELECT AUTEURNR
  FROM BOEKEN
  WHERE BOEKNR IN (
    SELECT BOEKNR
    FROM RESERVERINGEN ) ) ;
e)
SELECT VOORNAAM, TUSSENVOEGSEL, ACHTERNAAM
FROM LEERLINGEN
WHERE LLNR IN (
  SELECT LLNR
  FROM UITLENINGEN 
  WHERE BOEKNR IN (
    SELECT BOEKNR
    FROM BOEKEN
    WHERE TITEL = "Au pair" ) ) ;
7.2-----------------------------
a)
SELECT TITEL
FROM BOEKEN
WHERE NOT BOEKNR IN (
  SELECT BOEKNR
  FROM UITLENINGEN ) ;
b)
SELECT a.VOORNAAM, a.TUSSENVOEGSEL, a.ACHTERNAAM
FROM AUTEURS a, BOEKEN b
WHERE a.AUTEURNR = b.AUTEURNR
AND NOT b.BOEKNR IN (
    SELECT BOEKNR
    FROM EXEMPLAREN
    ) ;
================================
8.1-----------------------------
a)
SELECT a.VOORNAAM, a.TUSSENVOEGSEL, a.ACHTERNAAM, b.TITEL
FROM AUTEURS a, BOEKEN b
WHERE a.AUTEURNR = b.AUTEURNR
ORDER BY a.ACHTERNAAM ;
b)
SELECT l.VOORNAAM, l.TUSSENVOEGSEL, l.ACHTERNAAM, u.BOETE
FROM LEERLINGEN l, UITLENINGEN u
WHERE l.LLNR = u.LLNR
AND BOETE > 0 ;
c)
SELECT b.TITEL
FROM AUTEURS a, BOEKEN b
WHERE a.AUTEURNR = b.AUTEURNR
AND a.GEB_JAAR < 1900 ;
d)
SELECT a.VOORNAAM, a.TUSSENVOEGSEL, a.ACHTERNAAM
FROM AUTEURS a, BOEKEN b, EXEMPLAREN e
WHERE a.AUTEURNR = b.AUTEURNR
AND b.BOEKNR = e.BOEKNR
AND e.STATUS = "in reparatie"
e)
SELECT l.VOORNAAM, l.TUSSENVOEGSEL, l.ACHTERNAAM, b.TITEL, a.VOORNAAM, a.TUSSENVOEGSEL, a.ACHTERNAAM
FROM AUTEURS a, BOEKEN b, LEERLINGEN l, UITLENINGEN u
WHERE a.AUTEURNR = b.AUTEURNR
AND b.BOEKNR = u.BOEKNR
AND u.LLNR = l.LLNR
AND l.KLAS LIKE "6*"
================================
9.1-----------------------------
SELECT b.TITEL, e.UITLEENBAAR
FROM BOEKEN b, EXEMPLAREN e
WHERE b.BOEKNR = e.BOEKNR
AND b.TITEL LIKE "Giph*" ;

Ja hij kan het boek lenen.
9.2-----------------------------
a)
SELECT a.VOORNAAM, a.TUSSENVOEGSEL, a.ACHTERNAAM, b.TITEL, l.VOORNAAM, l.TUSSENVOEGSEL, l.ACHTERNAAM, l.KLAS
FROM AUTEURS a, BOEKEN b, LEERLINGEN l, UITLENINGEN u
WHERE l.LLNR = u.LLNR
AND u.BOEKNR = b.BOEKNR
AND b.AUTEURNR = a.AUTEURNR
AND u.DATUM_TERUG IS NULL ;
b)
SELECT a.VOORNAAM, a.TUSSENVOEGSEL, a.ACHTERNAAM, b.TITEL, l.VOORNAAM, l.TUSSENVOEGSEL, l.ACHTERNAAM, l.KLAS
FROM AUTEURS a, BOEKEN b, LEERLINGEN l, RESERVERINGEN r
WHERE a.AUTEURNR = b.AUTEURNR
AND b.BOEKNR = r.BOEKNR
AND r.LLNR = l.LLNR
AND r.STATUS = "beschikbaar"
c)
