FROM EXEMPLAREN e, BOEKEN b
WHERE e.STATUS = "in reparatie"
  AND e.BOEKNR = b.BOEKNR
  AND b.RUBRIEK = "Nederlands";
d)
SELECT VOORNAAM, TUSSENVOEGSEL, ACHTERNAAM, AUTEURNR DISTINCT
FROM AUTEURS
WHERE AUTEURNR IN (
  SELECT AUTEURNR
  FROM BOEKEN
  WHERE BOEKNR IN (
    SELECT BOEKNR
    FROM RESERVERINGEN ) ) ;
e)
SELECT VOORNAAM, TUSSENVOEGSEL, ACHTERNAAM
FROM LEERLINGEN
WHERE LLNR IN (
  SELECT LLNR
  FROM UITLENINGEN 
  WHERE BOEKNR IN (
    SELECT BOEKNR
    FROM BOEKEN
    WHERE TITEL = "Au pair" ) ) ;
