SO 1-5
1)
SELECT VOORNAAM, TUSSENVOEGSEL, ACHTERNAAM, KLAS, GEB_DATUM, GESLACHT, LLNR
FROM LEERLINGEN
ORDER BY KLAS ASC, GEB_DATUM DESC ;
2)
SELECT VOORNAAM, ACHTERNAAM, KLAS
FROM LEERLINGEN
WHERE ACHTERNAAM LIKE "Bos*" ;3)
a)
SELECT MAX(GEB_DATUM) 
FROM LEERLINGEN ;
b)
SELECT *
FROM LEERLINGEN
WHERE GEB_DATUM IN (
SELECT MAX(GEB_DATUM) 
FROM LEERLINGEN ) ;
4)
SELECT DISTINCT RUBRIEK
FROM BOEKEN
ORDER BY RUBRIEK DESC ;
5)
a)
SELECT b.TITEL, b.BOEKNR, b.PUB_JAAR, b.AUTEURNR
FROM AUTEURS a, BOEKEN b
WHERE a.AUTEURNR = b.AUTEURNR
AND a.ACHTERNAAM = "Hermans"
AND a.VOORNAAM = "Willem Frederik" ;
b)
SELECT TITEL, BOEKNR, PUB_JAAR, AUTEURNR 
FROM BOEKEN
WHERE AUTEURNR IN (
SELECT AUTEURNR
FROM AUTEURS
WHERE ACHTERNAAM = "Hermans"
AND VOORNAAM = "Willem Frederik" ) ;
6)
SELECT VOORNAAM, ACHTERNAAM, KLAS
FROM LEERLINGEN
WHERE POSTCODE LIKE "35*2E*" ;
7)
SELECT a.VOORNAAM, a.TUSSENVOEGSEL, a.ACHTERNAAM, b.TITEL
FROM BOEKEN b, AUTEURS a
WHERE AANTALPAGS > 150
AND AANTALPAGS < 200
AND a.AUTEURNR = b.AUTEURNR
ORDER BY AANTALPAGS ;
8)
SELECT VOORNAAM, TUSSENVOEGSEL, ACHTERNAAM, GEB_DATUM, KLAS
FROM LEERLINGEN
WHERE MONTH(GEB_DATUM) = 9
ORDER BY GEB_DATUM ASC ;
