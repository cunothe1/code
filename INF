OEFENBLAD
1)
SELECT ACHTERNAAM, KLAS, PLAATS
FROM LEERLINGEN
WHERE PLAATS = "Schalkwijk";
2)
SELECT LLNR, BOETE
FROM UITLENINGEN
WHERE BOETE = "2.00";
3)
SELECT TITEL
FROM BOEKEN
WHERE RUBRIEK = "Nederlands";
4)
SELECT AANTALPAGS
FROM BOEKEN
WHERE TITEL = "The Lord of the rings";
5)
SELECT TITEL, PUB_JAAR
FROM BOEKEN
WHERE PUB_JAAR > 1980;
6)
SELECT TITEL, PUB_JAAR
FROM BOEKEN
WHERE PUB_JAAR >= 1940 AND PUB_JAAR <= 1945;
7)
SELECT *
FROM LEERLINGEN
WHERE MONTH (GEB_DATUM) = 9
AND DAY (GEB_DATUM) >= 4
AND DAY (GEB_DATUM) <= 10
8)
SELECT TITEL
FROM BOEKEN
WHERE AANTALPAGS > 500;

SELECT B.TITEL, A.VOORNAAM, A.TUSSENVOEGSEL, A.ACHTERNAAM, B.AANTALPAGS
FROM BOEKEN B, AUTEURS A
WHERE B.AANTALPAGS > 500
AND A.AUTEURNR = B.AUTEURNR;
