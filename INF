8.1-----------------------------
a)
SELECT a.VOORNAAM, a.TUSSENVOEGSEL, a.ACHTERNAAM, b.TITEL
FROM AUTEURS a, BOEKEN b
WHERE a.AUTEURNR = b.AUTEURNR
ORDER BY a.ACHTERNAAM ;
b)
SELECT l.VOORNAAM, l.TUSSENVOEGSEL, l.ACHTERNAAM, u.BOETE
FROM LEERLINGEN l, UITLENINGEN u
WHERE l.LLNR = u.LLNR
AND BOETE > 0 ;
c)
SELECT b.TITEL
FROM AUTEURS a, BOEKEN b
WHERE a.AUTEURNR = b.AUTEURNR
AND a.GEB_JAAR < 1900 ;
d)
SELECT a.VOORNAAM, a.TUSSENVOEGSEL, a.ACHTERNAAM
FROM AUTEURS a, BOEKEN b, EXEMPLAREN e
WHERE a.AUTEURNR = b.AUTEURNR
AND b.BOEKNR = e.BOEKNR
AND e.STATUS = "in reparatie"
e)
SELECT l.VOORNAAM, l.TUSSENVOEGSEL, l.ACHTERNAAM, b.TITEL, a.VOORNAAM, a.TUSSENVOEGSEL, a.ACHTERNAAM
FROM AUTEURS a, BOEKEN b, LEERLINGEN l, UITLENINGEN u
WHERE a.AUTEURNR = b.AUTEURNR
AND b.BOEKNR = u.BOEKNR
AND u.LLNR = l.LLNR
AND l.KLAS LIKE "6*"
AND b.rubriek = "Nederlands"
AND u.DATUM_TERUG IS NULL;
